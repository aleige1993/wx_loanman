<!--pages/orderList/index.wxml-->
<view class='order_panl'>
    <view class='order_model' wx:for="{{orderList}}" wx:key="orderList">
        <view class='header'>
            <view>订单号：{{item.orderNo}}</view>
            <view>支付时间：{{item.payTime?item.payTime:0}}</view>
        </view>
        <view class='order_center'> 
            <view>服务名称：<text class='text-shadow {{item.productType == 1? "text-mauve" : "text-blue"}} '>{{item.productName}}</text></view>
             <view>成交价：<text class='text-shadow text-red'>{{item.fee}}元</text></view>
              <view>有效期：<text>{{item.endTime?item.endTime:0}}</text></view>
        </view>
    </view>
    <view class='nulldata' wx:if="{{orderList.length<=0}}">
        <icon class='iconfont iconqunfengweizhaodaoshangjia'></icon>
        <view class='tlte_null'>暂无数据~~~</view>
    </view>
</view>