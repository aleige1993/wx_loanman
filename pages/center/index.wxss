.user_info{
  background-color: #fff;
  padding:30rpx;
  border-radius:6rpx;
  display: flex;
}
.user_info image{
  width:100rpx;
  height: 100rpx;
  border-radius:100%; 
  margin-right: 20rpx;
}
.user_model{
  flex: 1;
  align-items: center;
  align-self: center;
  justify-content: space-between;
  align-content: space-between;
}
.level{
  width: 100%;
  display: flex;
  justify-content:space-between;
  align-content: center;
  align-items: center;
  align-self: center;
}
.days{
  margin-top: 20rpx;
}
.nechens icon{
  margin-left: 20rpx;
}

.ul_panl{
  background-color: #fff;
  border-radius:10rpx;
  margin-top: 30rpx;
}
.wx_li{
  display: flex;
  justify-content:space-between;
  padding:30rpx;
  border-bottom: 1px solid #eee;
}
.ul_panl .wx_li:last-child{
  border-bottom:none; 
} 
.wx_li .heder text{
  color: #8799a3;
  margin-left: 20rpx;
}
.b_g{
  height: 260rpx;
}

.nickname{
